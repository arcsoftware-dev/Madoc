meta {
  name: changePassword
  type: http
  seq: 3
}

put {
  url: {{host}}/auth/manage/password
  body: formUrlEncoded
  auth: inherit
}

body:json {
  {
    "username": {{admin_username}},
    "password": {{default_password}}
  }
}

body:form-urlencoded {
  username: admin
  oldPassword: {{default_password}}
  newPassword: {{admin_password}}
}

settings {
  encodeUrl: true
}
